<% function value(field, originalValue) { %>
<%   if (data === null) { %>
<%     return `value="${originalValue}"` %>
<%   } %>
<%  %>
<%   return `value="${data.fields[field]}"` %>
<% } %>

<% function ta(field, originalValue) { %>
<%   if (data === null) { %>
<%     return originalValue %>
<%   } %>
<%  %>
<%   return data.fields[field] %>
<% } %>

<% function opt(field, arrOptions, defaultOption) { %>
<%   let selectedOption = defaultOption %>
<%   if(data !== null) { %>
<%     selectedOption = data?.fields[field] %>
<%   } %>
<%   return arrOptions.map(option => (`<option ${option === '' ? 'class="empty-option"' : ''} value="${option}" ${selectedOption === option ? 'selected' : ''}>${option}</option>`)).join('') %>
<% } %>
  

